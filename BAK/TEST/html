  [<a href="#cereactions" id="the-param-element:cereactions-2"><c- g="">CEReactions</c-></a>] <c- b="">attribute</c-> <c- b="">DOMString</c-> <a href="#dom-param-value" id="the-param-element:dom-param-value"><c- g="">value</c-></a>;

