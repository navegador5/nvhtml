  [<a href="#cereactions" id="the-a-element:cereactions-2"><c- g="">CEReactions</c-></a>] <c- b="">attribute</c-> <c- b="">DOMString</c-> <a href="#dom-a-download" id="the-a-element:dom-a-download"><c- g="">download</c-></a>;
