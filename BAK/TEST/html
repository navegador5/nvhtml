  [<a href="#cereactions" id="the-select-element:cereactions-10"><c- g="">CEReactions</c-></a>] <c- b="">undefined</c-> <a href="#dom-select-remove" id="the-select-element:dom-select-remove-2"><c- g="">remove</c-></a>(<c- b="">long</c-> <c- g="">index</c->);
