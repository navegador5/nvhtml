  [<a href="#cereactions" id="the-area-element:cereactions-4"><c- g="">CEReactions</c-></a>] <c- b="">attribute</c-> <c- b="">DOMString</c-> <a href="#dom-area-target" id="the-area-element:dom-area-target"><c- g="">target</c-></a>;
